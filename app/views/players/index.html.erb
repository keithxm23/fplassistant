<%- model_class = Player -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:first_name) %></th>
      <th><%= model_class.human_attribute_name(:second_name) %></th>
      <th><%= model_class.human_attribute_name(:elementid) %></th>
      <th><%= model_class.human_attribute_name(:transfers_out) %></th>
      <th><%= model_class.human_attribute_name(:code) %></th>
      <th><%= model_class.human_attribute_name(:event_total) %></th>
      <th><%= model_class.human_attribute_name(:last_season_points) %></th>
      <th><%= model_class.human_attribute_name(:squad_number) %></th>
      <th><%= model_class.human_attribute_name(:transfers_balance) %></th>
      <th><%= model_class.human_attribute_name(:news_updated) %></th>
      <th><%= model_class.human_attribute_name(:news_added) %></th>
      <th><%= model_class.human_attribute_name(:event_cost) %></th>
      <th><%= model_class.human_attribute_name(:web_name) %></th>
      <th><%= model_class.human_attribute_name(:in_dreamteam) %></th>
      <th><%= model_class.human_attribute_name(:team_code) %></th>
      <th><%= model_class.human_attribute_name(:shirt_image_url) %></th>
      <th><%= model_class.human_attribute_name(:transfers_out_event) %></th>
      <th><%= model_class.human_attribute_name(:element_type_id) %></th>
      <th><%= model_class.human_attribute_name(:max_cost) %></th>
      <th><%= model_class.human_attribute_name(:selected) %></th>
      <th><%= model_class.human_attribute_name(:min_cost) %></th>
      <th><%= model_class.human_attribute_name(:total_points) %></th>
      <th><%= model_class.human_attribute_name(:type_name) %></th>
      <th><%= model_class.human_attribute_name(:team_name) %></th>
      <th><%= model_class.human_attribute_name(:status) %></th>
      <th><%= model_class.human_attribute_name(:added) %></th>
      <th><%= model_class.human_attribute_name(:form) %></th>
      <th><%= model_class.human_attribute_name(:shirt_mobile_image_url) %></th>
      <th><%= model_class.human_attribute_name(:current_fixture) %></th>
      <th><%= model_class.human_attribute_name(:now_cost) %></th>
      <th><%= model_class.human_attribute_name(:points_per_game) %></th>
      <th><%= model_class.human_attribute_name(:transfers_in) %></th>
      <th><%= model_class.human_attribute_name(:news) %></th>
      <th><%= model_class.human_attribute_name(:original_cost) %></th>
      <th><%= model_class.human_attribute_name(:event_points) %></th>
      <th><%= model_class.human_attribute_name(:news_return) %></th>
      <th><%= model_class.human_attribute_name(:next_fixture) %></th>
      <th><%= model_class.human_attribute_name(:transfers_in_event) %></th>
      <th><%= model_class.human_attribute_name(:selected_by) %></th>
      <th><%= model_class.human_attribute_name(:team_id) %></th>
      <th><%= model_class.human_attribute_name(:photo_mobile_url) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @players.each do |player| %>
      <tr>
        <td><%= link_to player.id, player_path(player) %></td>
        <td><%= player.first_name %></td>
        <td><%= player.second_name %></td>
        <td><%= player.elementid %></td>
        <td><%= player.transfers_out %></td>
        <td><%= player.code %></td>
        <td><%= player.event_total %></td>
        <td><%= player.last_season_points %></td>
        <td><%= player.squad_number %></td>
        <td><%= player.transfers_balance %></td>
        <td><%= player.news_updated %></td>
        <td><%= player.news_added %></td>
        <td><%= player.event_cost %></td>
        <td><%= player.web_name %></td>
        <td><%= player.in_dreamteam %></td>
        <td><%= player.team_code %></td>
        <td><%= player.shirt_image_url %></td>
        <td><%= player.transfers_out_event %></td>
        <td><%= player.element_type_id %></td>
        <td><%= player.max_cost %></td>
        <td><%= player.selected %></td>
        <td><%= player.min_cost %></td>
        <td><%= player.total_points %></td>
        <td><%= player.type_name %></td>
        <td><%= player.team_name %></td>
        <td><%= player.status %></td>
        <td><%= player.added %></td>
        <td><%= player.form %></td>
        <td><%= player.shirt_mobile_image_url %></td>
        <td><%= player.current_fixture %></td>
        <td><%= player.now_cost %></td>
        <td><%= player.points_per_game %></td>
        <td><%= player.transfers_in %></td>
        <td><%= player.news %></td>
        <td><%= player.original_cost %></td>
        <td><%= player.event_points %></td>
        <td><%= player.news_return %></td>
        <td><%= player.next_fixture %></td>
        <td><%= player.transfers_in_event %></td>
        <td><%= player.selected_by %></td>
        <td><%= player.team_id %></td>
        <td><%= player.photo_mobile_url %></td>
        <td><%=l player.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_player_path(player), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      player_path(player),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_player_path,
            :class => 'btn btn-primary' %>
